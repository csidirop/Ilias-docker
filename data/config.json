{
    "common" : {
        "client_id" : "testilias",
        "server_timezone" : "Europe/Berlin"
    },
    "database" : {
        "type" : "innodb",
        "host" : "db",
        "user" : "ilias",
        "password" : "dbpassword",
        "create_database" : true
    },
    "filesystem" : {
        "data_dir" : "/var/www/files"
    },
